<% provide(:title, 'Log in' )%>
<h1 class="text-center">Login</h1>
<%= form_with(url: login_path, scope: :session) do |f|%>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password%>
  <%= f.submit "Login" %>
<% end %>
<p class="text-center">New User ? <%= link_to 'Sign Up Now', signup_path %></p>